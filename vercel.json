{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api-docs/(.*)",
            "dest": "app.js",
            "headers": {
                "Content-Security-Policy": "script-src 'self' https://unpkg.com 'unsafe-inline'; style-src 'self' 'unsafe-inline' https://unpkg.com;"
            }
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}